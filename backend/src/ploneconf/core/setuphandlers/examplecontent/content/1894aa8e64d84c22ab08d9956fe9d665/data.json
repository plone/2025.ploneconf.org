    "2ee57b7f-2071-4c88-b25c-9f156091541b": {
      "@type": "slate",
      "plaintext": "This year, in cooperation with active pythonistas in Finland, we are bringing back PyCon Finland ! Inside the week-long Plone Conference , we dedicate Friday October 17th , with dedicated single-day ticket , to cater content specifically to pythonistas out there.",
      "styles": {},
      "theme": "default",
      "value": [
        {
          "children": [
            {
              "text": "This year, in cooperation with active pythonistas in Finland, we are bringing back "
            },
            {
              "children": [
                {
                  "text": "PyCon Finland"
                }
              ],
              "type": "strong"
            },
            {
              "text": "! Inside the "
            },
            {
              "children": [
                {
                  "text": "week-long Plone Conference"
                }
